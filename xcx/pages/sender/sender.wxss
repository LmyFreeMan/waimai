page {
  background-color: #eee;
}

.header {
  display: flex;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  background-color: #fff;
  position: fixed;
  width: 750rpx;
}

.header-item {
  flex: 1;
}

.header-selected {
  color: #0097fe;
}

.content {
  padding-top: 80rpx;
}

.item {
  background-color: #fff;
  margin-top: 20rpx;
}

.item-order {
}

.item-order-top {
  height: 120rpx;
  width: 750rpx;
}

.item-order-top-img {
  width: 65rpx;
  height: 65rpx;
  float: left;
  margin-left: 29rpx;
  margin-top: 27rpx;
  border: 2rpx solid #eee;
  border-radius: 8rpx;
}

.item-order-top-right {
  float: left;
  height: 120rpx;
  width: 634rpx;
  line-height: 120rpx;
  margin-left: 20rpx;
  border-bottom: 2rpx solid #eee;
}

.item-order-top-right-top {
  height: 80rpx;
  line-height: 80rpx;
  /* display: flex;
  align-items: center; */
}

.item-order-top-right-top-text {
  font-size: 30rpx;
  width: 350rpx;
  float: left;
}

.item-order-top-right-top-more {
  width: 15rpx;
  height: 30rpx;
}

.item-order-top-right-top-right-btn {
  height: 60rpx;
  font-size: 26rpx;
  float: right;
  margin-right: 29rpx;
  margin-top: 10rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #fff;
}

.text-color1 {
  color: red;
}

.item-order-top-right-top-right {
  font-size: 26rpx;
  float: right;
  margin-right: 29rpx;
}

.item-order-top-right-time {
  line-height: 20rpx;
  font-size: 16rpx;
}

.item-msg {
  height: 87rpx;
  line-height: 87rpx;
  margin-left: 116rpx;
}

.item-msg-desc {
  font-size: 24rpx;
  color: #666;
}

.item-msg-price {
  font-size: 26rpx;
  float: right;
  margin-right: 29rpx;
}

.weui-loading {
  margin: 0 10rpx;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 24rpx;
  text-align: center;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
